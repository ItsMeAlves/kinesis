<Window x:Class="Kinesis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kinesis"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="angleInput" HorizontalAlignment="Left" Height="23" Margin="269,286,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <ListBox x:Name="portInput" HorizontalAlignment="Left" Height="100" Margin="269,154,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="portBtn" Content="Connect" HorizontalAlignment="Left" Margin="425,166,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="portUpdate" Content="Update List" HorizontalAlignment="Left" Margin="425,213,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="angleBtn" Content="Set Angle" HorizontalAlignment="Left" Margin="425,254,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
